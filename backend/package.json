{
  "name": "dental-chatbot-backend",
  "version": "1.0.0",
  "description": "Express.js backend API for Dental AI Chatbot with JWT authentication and appointment management",
  "private": true,
  "type": "module",
  "main": "server.js",
  "author": "Dental AI Team",
  "license": "MIT",
  "keywords": [
    "dental",
    "chatbot",
    "api",
    "express",
    "authentication",
    "appointments"
  ],
  "scripts": {
    "dev": "node --watch server.js",
    "start": "node server.js",
    "test": "jest --detectOpenHandles --forceExit",
